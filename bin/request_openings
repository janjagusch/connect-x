#! /bin/bash

# Downloads the connect 4 openings from the UCI machine learning repository,
# uncompresses it and stores it in data/openings/openings.csv

wget \
    --no-check-certificate \
    --quiet \
    --output-document - \
    "https://archive.ics.uci.edu/ml/machine-learning-databases/connect-4/connect-4.data.Z" \
    | uncompress \
    >> data/openings/openings.csv
